[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/nvngithub007
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/nvngithub007
	label-Verified = -1..+1 group user/nvngithub007
	push = group user/nvngithub007
	read = group user/nvngithub007
	rebase = group user/nvngithub007
	removeReviewer = group user/nvngithub007
	submit = group user/nvngithub007
[access "refs/heads/*"]
	create = group user/nvngithub007
	push = +force group user/nvngithub007
	pushMerge = group user/nvngithub007
	read = group user/nvngithub007
[access "refs/tags/*"]
	push = group user/nvngithub007
	push = +force group user/nvngithub007
